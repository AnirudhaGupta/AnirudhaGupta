#include <iostream>
using namespace std;
int sum(int arr[],int size){
  int sum = 0;
    for (int i= 0 ; i<size; i++){
      int a = arr[i];
      sum = sum + a;
    }
  return sum;
}
int main() {
  
  int size;
  cout<<"enter value for size : ";
  cin>>size;
  int arr[size];
  for (int i = 0;i<size;i++){
    cin>>arr[i];  
  }
  cout << sum(arr,size);
}
